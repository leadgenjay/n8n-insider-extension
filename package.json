{
  "name": "n8n-insider",
  "private": true,
  "workspaces": [
    "extension",
    "web"
  ],
  "scripts": {
    "dev:ext": "npm run dev -w extension",
    "dev:web": "npm run dev -w web",
    "dev": "concurrently \"npm run dev:ext\" \"npm run dev:web\"",
    "build:ext": "npm run build -w extension",
    "build:web": "npm run build -w web",
    "build": "npm run build:ext && npm run build:web",
    "lint": "npm run lint -w web"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
